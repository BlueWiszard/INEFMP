;���Squads define���

(define "squad_with1types"
	{"%name(%side)"
		{charge 
			{delay %cd}
			{interval %ci}
		}
		{content "mp/%side/%c1"}
		{tags "%side %t"}
		{group "%g"}
		{button "%b"}
		{level %level}
		{cost %cost}
		{research_stage %research_stage}
		{squad_cost_factor %scf}
	}
)

